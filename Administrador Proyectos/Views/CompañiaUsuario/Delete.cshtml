@model Administrador_Proyectos.Models.CompañiaUsuario

@{
    ViewData["Title"] = "Borrando";
}

<h2>Borrando</h2>

<h3>¿Estas seguro de borrar esto?</h3>
<div>
    <h4>CompañiaUsuario</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Compañia)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Compañia.Nombre)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Usuario.Nombre)
        </dd>
        <dt>
            Fecha de asignación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaAsignacion)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-default" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
